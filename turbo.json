{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["NODE_ENV"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "db:push": {
      "outputs": []
    },
    "db:generate": {
      "outputs": []
    },
    "db:migrate": {
      "outputs": []
    },
    "db:reset": {
      "outputs": []
    }
  }
}